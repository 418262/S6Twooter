services:
  gateway:
    build:
      context: C:\Users\pietv\source\repos\Twooter2
      dockerfile: Gateway/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: Gateway
      target: base
    container_name: Gateway_1
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ASPNETCORE_URLS: https://+:443;http://+:80
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: /root/.nuget/fallbackpackages
    image: gateway:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages
        --additionalProbingPath /root/.nuget/fallbackpackages  "/app/bin/Debug/net6.0/Gateway.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - target: 80
    - target: 443
    tty: true
    volumes:
    - /c/Users/pietv/source/repos/Twooter2/Gateway:/app:rw
    - /c/Users/pietv/vsdbg/vs2017u5:/remote_debugger:rw
    - /c/Users/pietv/AppData/Roaming/ASP.NET/Https:/root/.aspnet/https:ro
    - /c/Users/pietv/AppData/Roaming/Microsoft/UserSecrets:/root/.microsoft/usersecrets:ro
    - /c/Program Files/dotnet/sdk/NuGetFallbackFolder:/root/.nuget/fallbackpackages:ro
    - /c/Users/pietv/.nuget/packages:/root/.nuget/packages:ro
    - /c/Users/pietv/source/repos/Twooter2:/src:rw
  userservice:
    build:
      context: C:\Users\pietv\source\repos\Twooter2
      dockerfile: UserService/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: UserService
      target: base
    container_name: UserService_1
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: /root/.nuget/fallbackpackages
    image: userservice:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages
        --additionalProbingPath /root/.nuget/fallbackpackages  "/app/bin/Debug/net6.0/UserService.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    tty: true
    volumes:
    - /c/Users/pietv/source/repos/Twooter2/UserService:/app:rw
    - /c/Users/pietv/vsdbg/vs2017u5:/remote_debugger:rw
    - /c/Users/pietv/AppData/Roaming/ASP.NET/Https:/root/.aspnet/https:ro
    - /c/Users/pietv/AppData/Roaming/Microsoft/UserSecrets:/root/.microsoft/usersecrets:ro
    - /c/Program Files/dotnet/sdk/NuGetFallbackFolder:/root/.nuget/fallbackpackages:ro
    - /c/Users/pietv/.nuget/packages:/root/.nuget/packages:ro
    - /c/Users/pietv/source/repos/Twooter2:/src:rw
version: '3'